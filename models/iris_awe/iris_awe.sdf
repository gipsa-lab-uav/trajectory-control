<?xml version='1.0'?>
<sdf version='1.4'>

  <model name='iris_awe'>

    <self_collide>true</self_collide>

    <include>
      <uri>model://iris</uri>
      <pose>0 0 1.06 0 0 0</pose>
    </include>

    <include>
      <uri>model://anchor_point</uri>
      <pose>0 0 1.01 0 0 0</pose>
    </include>

    <include>
      <uri>model://winch_station</uri>
      <pose>0 0 0.5 0 0 0</pose>
    </include>

    <joint name="anchor_joint" type="fixed">
      <parent>iris::base_link</parent>
      <child>anchor_point::base_link</child>
    </joint>

    <!-- <joint name="cable_joint" type="universal">
      <parent>winch_station::base_link</parent>
      <child>anchor_point::base_link</child>
    </joint> -->

    <plugin name='cable_model' filename='libcable_plugin.so'>
      <robotNamespace/>
      <jointName>anchor_joint</jointName>
      <linkName>anchor_point::base_link</linkName>
    </plugin>

  </model>
</sdf>
