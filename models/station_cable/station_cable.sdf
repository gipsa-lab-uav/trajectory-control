<?xml version='1.0'?>
<sdf version='1.4'>
  <model name='station_cable'>
    <self_collide>false</self_collide>

    <link name="station_link">
      <self_collide>true</self_collide>
      <pose>0 0 0 0 0 0</pose>
      <inertial>
        <mass>100</mass>
      </inertial>
      <collision name="station_link_collision">
        <geometry>
          <box>
            <size>1 1 1</size>
          </box>
        </geometry>
        <surface>
          <contact>true</contact>
        </surface>
      </collision>
      <visual name='station_link_visual'>
        <geometry>
          <box>
            <size>1 1 1</size>
          </box>
        </geometry>
      </visual>
    </link>

    <link name="cable_link">
      <pose>0 0 0.5 0 0 0</pose>
      <inertial>
        <mass>0</mass>
      </inertial>
      <visual name='cable_link_visual'>
        <geometry>
          <box>
            <size>0.01 0.01 0.01</size>
          </box>
        </geometry>
      </visual>
    </link>

    <joint name="station_cable_joint" type="universal">
      <parent>station_link</parent>
      <child>cable_link</child>
      <pose>0 1 0 0 0 0</pose>
    </joint>

  </model>
</sdf>
