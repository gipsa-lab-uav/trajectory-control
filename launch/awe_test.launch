<launch>
	<arg name="simulated-env" 	default="true"	/>
	<arg name="trajectory-gen" 	default="false"	/>
	<arg name="world" default="$(find mavlink_sitl_gazebo)/worlds/empty.world"/>
	<arg name="vehicle" default="iris_awe"/>

    <!-- STARTS CODE FROM mavros_posix_sitl.launch -->
		<arg name="x" default="0"/>
		<arg name="y" default="0"/>
		<arg name="z" default="0"/>
		<arg name="R" default="0"/>
		<arg name="P" default="0"/>
		<arg name="Y" default="0"/>
		<!-- vehicle model and world -->
		<arg name="est" default="ekf2"/>
		<arg name="sdf" default="$(find mavlink_sitl_gazebo)/models/$(arg vehicle)/$(arg vehicle).sdf"/>

		<!-- gazebo configs -->
		<arg name="gui" default="true"/>
		<arg name="debug" default="false"/>
		<arg name="verbose" default="false"/>
		<arg name="paused" default="false"/>
		<arg name="respawn_gazebo" default="false"/>
		<!-- MAVROS configs -->
		<arg name="fcu_url" default="udp://:14540@localhost:14557"/>
		<arg name="respawn_mavros" default="false"/>
		<!-- PX4 configs -->
		<arg name="interactive" default="true"/>
		<!-- PX4 SITL and Gazebo -->
		<include file="$(find px4)/launch/posix_sitl.launch">
			<arg name="x" value="$(arg x)"/>
			<arg name="y" value="$(arg y)"/>
			<arg name="z" value="$(arg z)"/>
			<arg name="R" value="$(arg R)"/>
			<arg name="P" value="$(arg P)"/>
			<arg name="Y" value="$(arg Y)"/>
			<arg name="world" value="$(arg world)"/>
			<arg name="vehicle" value="$(arg vehicle)"/>
			<arg name="sdf" value="$(arg sdf)"/>
			<arg name="gui" value="$(arg gui)"/>
			<arg name="interactive" value="$(arg interactive)"/>
			<arg name="debug" value="$(arg debug)"/>
			<arg name="verbose" value="$(arg verbose)"/>
			<arg name="paused" value="$(arg paused)"/>
			<arg name="respawn_gazebo" value="$(arg respawn_gazebo)"/>
		</include>
		<!-- MAVROS -->
		<include file="$(find mavros)/launch/px4.launch">
			<!-- GCS link is provided by SITL -->
			<arg name="gcs_url" value=""/>
			<arg name="fcu_url" value="$(arg fcu_url)"/>
			<arg name="respawn_mavros" value="$(arg respawn_mavros)"/>
		</include>
	<!-- ENDS CODE FROM mavros_posix_sitl.launch -->

	<node pkg="trajectory_control" type="trajectory_control_node" name="trajectory_control_node_sitl" output="screen" if="$(arg simulated-env)">
		<param name="fsf_x_P" value="0.3" />
		<param name="fsf_x_S" value="2.5" />
		<param name="fsf_y_P" value="0.3" />
		<param name="fsf_y_S" value="2.5" />
		<param name="fsf_z_P" value="0.3" />
		<param name="fsf_z_S" value="2.5" />

		<param name="use_StatesObserver" value="true" />

		<param name="se_x_Lpos" value="1.03" />
		<param name="se_y_Lpos" value="1.03" />
		<param name="se_z_Lpos" value="1.03" />
		<param name="se_x_Lspeed" value="10.7" />
		<param name="se_y_Lspeed" value="10.7" />
		<param name="se_z_Lspeed" value="10.7" />
		<param name="se_x_Lunc" value="9.62" />
		<param name="se_y_Lunc" value="9.62" />
		<param name="se_z_Lunc" value="9.62" />

		<param name="se_x_Filter" value="0.95" />
		<param name="se_y_Filter" value="0.95" />
		<param name="se_z_Filter" value="0.15" />

		<param name="mass" value="1.5" />
		<param name="hoverCompensation" value="0.45" />
		<param name="maxAngle" value="45.0" />
		<param name="maxVerticalAcceleration" value="4.0" />
	</node>

  	<node pkg="trajectory_control" type="trajectory_gen.py" name="trajectory_gen_node" if="$(arg trajectory-gen)"></node>

</launch>
