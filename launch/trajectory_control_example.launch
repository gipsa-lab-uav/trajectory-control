<launch>
	<arg name="simulated_env" default="true" />
	<arg name="trajectory_gen" default="true" />
	<arg name="display" default="true" />
	<arg name="rosbag_record" default="false" />

	<arg name="world" default="$(find mavlink_sitl_gazebo)/worlds/empty.world"/>

	<include file="$(find px4)/launch/mavros_posix_sitl.launch" if="$(arg simulated_env)">
		<arg name="world" value="$(arg world)"/>
	</include>

	<node pkg="trajectory_control" type="trajectory_control_node" name="trajectory_control_node_sitl" output="screen" if="$(arg simulated_env)">
		<param name="simulated_env" value="$(arg simulated_env)" />
		<param name="ctrl_freq" value="100" />

		<param name="fsf_x_P" value="0.3" />
		<param name="fsf_x_S" value="2.5" />
		<param name="fsf_y_P" value="0.3" />
		<param name="fsf_y_S" value="2.5" />
		<param name="fsf_z_P" value="0.3" />
		<param name="fsf_z_S" value="2.5" />

		<param name="use_StatesObserver" value="false" />

		<!-- Lpos=1.03 Lspeed=10.7 Lunc=9.62-->
		<param name="se_x_Lpos" value="0.67" />
		<param name="se_y_Lpos" value="0.67" />
		<param name="se_z_Lpos" value="0.67" />
		<param name="se_x_Lspeed" value="3.4" />
		<param name="se_y_Lspeed" value="3.4" />
		<param name="se_z_Lspeed" value="3.4" />
		<param name="se_x_Lunc" value="6.0" />
		<param name="se_y_Lunc" value="6.0" />
		<param name="se_z_Lunc" value="6.0" />

		<param name="se_x_Filter" value="0.95" />
		<param name="se_y_Filter" value="0.95" />
		<param name="se_z_Filter" value="0.15" />

		<param name="alpha_vx" value="0.0" />
		<param name="alpha_vy" value="0.0" />
		<param name="alpha_vz" value="0.0" />
		<param name="alpha_ax" value="0.05" />
		<param name="alpha_ay" value="0.05" />
		<param name="alpha_az" value="0.05" />

		<param name="mass" value="1.5" />
		<param name="hoverCompensation" value="0.45" />
		<param name="maxAngle" value="45.0" />
		<param name="maxVerticalAcceleration" value="4.0" />
	</node>

	<node pkg="trajectory_control" type="trajectory_control_node" name="trajectory_control_node_real" output="screen" unless="$(arg simulated_env)">
		<param name="simulated_env" value="$(arg simulated_env)" />
		<param name="ctrl_freq" value="100" />

		<param name="fsf_x_P" value="1.0" />
		<param name="fsf_x_S" value="1.7" />
		<param name="fsf_y_P" value="1.0" />
		<param name="fsf_y_S" value="1.7" />
		<param name="fsf_z_P" value="1.56" />
		<param name="fsf_z_S" value="2.4" />

		<param name="use_StatesObserver" value="true" />

		<param name="se_x_Lpos" value="1.03" />
		<param name="se_y_Lpos" value="1.03" />
		<param name="se_z_Lpos" value="1.03" />
		<param name="se_x_Lspeed" value="10.7" />
		<param name="se_y_Lspeed" value="10.7" />
		<param name="se_z_Lspeed" value="10.7" />
		<param name="se_x_Lunc" value="9.62" />
		<param name="se_y_Lunc" value="9.62" />
		<param name="se_z_Lunc" value="9.62" />

		<param name="se_x_Filter" value="0.9" />
		<param name="se_y_Filter" value="0.9" />
		<param name="se_z_Filter" value="0.15" />

		<param name="mass" value="1.0" />
		<param name="hoverCompensation" value="0.3" />
		<param name="maxAngle" value="45.0" />
		<param name="maxVerticalAcceleration" value="4.0" />
	</node>

  <node pkg="trajectory_control" type="trajectory_gen.py" name="trajectory_gen_node" if="$(arg trajectory_gen)"></node>

	<node pkg="rosbag" type="record" name="rosbag_record_cam"
		args="record -o $(find trajectory_control)/rosbag/bag mavros/local_position/pose mavros/imu/data mavros/rc/out mavros/setpoint_raw/attitude"
		if="$(arg rosbag_record)"></node>

	<node pkg="trajectory_control" type="display.py" name="display_node" if="$(arg display)"></node>

</launch>
